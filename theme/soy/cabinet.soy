          <script>
            function send_validation(slot, message) {
              $("#loading").attr("src", "/img/loading.gif");
              $.ajax({
                type: "POST",
                url: "/u/api/send-validation",
                data: {slot: slot},
                success : function(data) {
                  var obj = $.parseJSON(data);
                  setAlertClass("profile-alert", "alert-success");
                  $("#profile-alert").text(message);
                },
                error : function(data) {
                  setAlertClass("profile-alert", "alert-error");
                  $("#profile-alert").text("Что-то пошло не так");
                },
                complete : function(data) {
                  $("#loading").attr("src", "");
                }
              });
            };
			$("#confirm-email").click(function(){
              send_validation("email", "Ссылка на подтверждение почтового адреса выслана");
            });
			$("#confirm-phone").click(function(){
              send_validation("phone", "СМС с кодом подтверждения выслано");
            });
			$("#confirm-bonuscard").click(function(){
              send_validation("bonuscard", "ok");
            });
            $("form#profile-form").submit(function(){
              $("#profile-form").submit(null);
              $("#loading").attr("src", "/img/loading.gif");
              $.ajax({
                type: "POST",
                url: "/u/api/profile",
                data: {
                  name: $("input[name='name']").val(),
                  birthdate: $("input[name='birthdate']").val(),
                  city: $("input[name='city']").val(),
                  address: $("input[name='address']").val(),
                  phone: $("input[name='phone']").val(),
                  bonuscard: $("input[name='bonuscard']").val(),
                  pass1: $("input[name='pass1']").val(),
                  pass2: $("input[name='pass2']").val()
                },
                success : function(data) {
                  var obj = $.parseJSON(data);
                  if (obj.error == "1") {
