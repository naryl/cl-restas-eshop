<div class="content" id="middle">
	<div class="full">
		<div class="left">
		{$menu | noAutoescape}
		</div>
		<div class="center">
			<div id="login-page">
				<span id="login-popup-title">Восстановление пароля</span>
				<div id="recovery-alert" class="alert {if $msg}alert-error{else}alert-none{/if}">
					{$msg}
				</div>
				<form id="reg-form" action="#">
					<span class="popup-label">E-mail:</span>
					<input type="text" name="username">
					<br>
					<div style="text-align:center;">
						<input type="submit" name="recover" value="Восстановить">
                        <img id="loading" width="32" height="32" />
					</div>
				</form>
				{literal}
				<script>
				$("form#reg-form").submit(function(){
					var error=0;
					if(error==0){
                        var email = $("form#reg-form").find("input[type='text']").val();
                        $("#loading").attr("src", "/img/loading.gif");
                        $.ajax({
                                type: "POST",
                                url: "/u/api/reset",
                                data: {
                                    username: email
                                },
                                success : function(data) {
                                    var obj = $.parseJSON(data);
                                    if (obj.error == 1) {
                                        setAlertClass("recovery-alert", "alert-error");
                                    } else {
                                        setAlertClass("recovery-alert", "alert-success");
                                    };
                                    $("#recovery-alert").text(obj.message);
                                },
                                complete : function(data) {
                                  $("#loading").attr("src", "");
                                }

                        });
						return false;
					}
					else{
						if(error==1) var err_text = "<p style='color:red;padding-left:20px;'>Проверьте заполнение выделенных полей!</p>";
						$(this).find(".messenger").html(err_text);
						return false;
					}
				});
				</script>
				{/literal}
			</div>
		</div>
		<div style="clear:both;"></div>
	</div>
</div>
{/template}

