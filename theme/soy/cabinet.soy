{template order }
/*  Заказ номер <b> </b><br/>{\n}
  адрес <b>{if $ADDRESS}{call address data="$ADDRESS" /}{/if}</b> <br/> {\n}
  сумма бонусов <b>{$BONUSCOUNT} </b><br/> {\n}
  комментарий к заказу <b>{$COMMENT} </b><br/> {\n}
  статус заказа  <b>{$order['ORDER-STATE']} </b> <br/> {\n}
  дата создания в виде timestamp <b>{$DATE} </b>  <br/> {\n}
  стоимость доставки <b>{$DELIVERY} </b>   <br/> {\n}
  номер <b>{$EKK} </b>          <br/> {\n}
  email <b>{$EMAIL} </b>  <br/> {\n}
  список товаров  <b>{$ITEMS} </b>   <br/> {\n}
  когда был модифицирован  <b>{$MODIFIED} </b>   <br/> {\n}
  номер телефона  <b>{$PHONE} </b> <br/> {\n}
  аккаунт   <b>{if $USER}{call user data="$USER" /}{/if} </b> <br/> {\n}
  фамилия   <b>{$USERFAMILY} </b> <br/> {\n}
  имя     <b>{$USERNAME} </b> <br/> {\n}*/
<div class="order-item">
	<div class="td col-1">
		<a class="open">{$order.KEY}</a>
		<a class="close" style="display:none;">{$order.KEY}</a>
	</div>
	<div class="td col-2">{date($order.DATE)}</div>
	<div class="td col-6">
	{switch $order['ORDER-STATE']}
		{case 1}
		<span class="table-status stadia-one"></span> Обработка
		{case 2}
		<span class="table-status stadia-two"></span> Принят
		{case 3}
		<span class="table-status stadia-three"></span> Отправка
		{case 4}
		<span class="table-status stadia-four"></span> Отгружен
		{default}
		<span class="table-status stadia-one"></span> Обработка
	{/switch}
	</div>
	<div class="col-5">
	{foreach $item in $order.ITEMS}
		{if $item}
			{$item['SITE-PRICE']} р.<br>
		{/if}
	{/foreach}
	</div>
	<div class="col-4">
	{foreach $item in $order.ITEMS}
		{if $item}
			{$item.COUNT} шт.<br>
		{/if}
	{/foreach}
	</div>
	<div class="col-3">
	{foreach $item in $order.ITEMS}
		{if $item}
			<a href="http://www.320-8080.ru/{$item.ARTICLE}">{$item.NAME}</a><br>
		{/if}
	{/foreach}
	</div>
	<div style="clear:both;"></div>
</div>
<div class="order-item-hidden">
	<div class="order-item-inner">
		{if $order.USER}
			<span class="info-span-label">Пользователь:</span> <span class="info-span">{call user data="$order.USER" /}</span><br>
		{/if}
		<span class="info-span-label">Фамилия, Имя:</span> <span class="info-span">{$order.USERFAMILY} {$order.USERNAME}</span><br>
		{if $order.PHONE}
			<span class="info-span-label">Номер телефона:</span> <span class="info-span">{$order.PHONE} </span><br>
		{/if}
		{if $order.EMAIL}
			<span class="info-span-label">E-mail:</span> <span class="info-span"><a href="mailto:{$order.EMAIL}">{$order.EMAIL}</a></span><br>
		{/if}
		{if $order.ADDRESS}
			<span class="info-span-label">Адрес:</span> <span class="info-span">{call address data="$order.ADDRESS" /}</span><br>
		{/if}
		{if $order.EKK}
			<span class="info-span-label">Номер бонусной карты:</span> <span class="info-span">{$order.EKK}</span><br>
		{/if}
		{if $order.BONUSCOUNT}
			<span class="info-span-label">Cумма бонусов:</span> <span class="info-span">{$order.BONUSCOUNT} </span><br>
		{/if}
		{if $order.DELIVERY}
			<span class="info-span-label">Cтоимость доставки:</span> <span class="info-span">{$order.DELIVERY} p.</span><br>
		{/if}
		{if $order.COMMENT}
			<span class="info-span-label">Комментарий к заказу:</span>
			<div class="info-comment">{$order.COMMENT}</div>
		{/if}
		/* !--когда был модифицирован  <b>{$order.MODIFIED} </b><br>-- */
	</div>
</div>
{/template}

