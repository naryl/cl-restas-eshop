{template alerts}
  {literal}
    <style>
.alert {
	padding: 8px 14px;
	margin:0 20px 18px;
	text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
}
.alert-none {
    display:none
}
.alert-error {
    display:block;
	color: #b94a48;
	background-color: #f2dede;
	border-color: #eed3d7;
}
.alert-success {
    display:block;
	color: #4ab948;
	background-color: #def2de;
	border-color: #d3eed7;
}
    </style>
    <script>
      function setAlertClass(id, cls) {
        $("#"+id).removeClass("alert-none alert-error alert-success");
        $("#"+id).removeClass("alert-none alert-error alert-success");
        $("#"+id).addClass(cls);
      }
    </script>
  {/literal}
{/template}
